{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mdcovidreportdf"
		},
		"ls_ablob_mdcovidreportingas_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_mdcovidreportingas'"
		},
		"ls_adls_mdcovidreportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_mdcovidreportingdl'"
		},
		"ls_db_covid_cluster_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_db_covid_cluster'"
		},
		"ls_sql_covid_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_covid_db'"
		},
		"ls_adls_mdcovidreportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mdcovidreportdl.dfs.core.windows.net"
		},
		"ls_http_ecdc_europa_eus_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tr_population_data_arrived_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/e109dae8-4906-4696-9e2a-348de044f8c0/resourceGroups/mdcovidreportrg/providers/Microsoft.Storage/storageAccounts/mdcovidreportsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mdcovidreportdf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_mdcovidreportingas_connectionString": {
						"value": "[parameters('ls_ablob_mdcovidreportingas_connectionString')]"
					},
					"ls_adls_mdcovidreportingdl_accountKey": {
						"value": "[parameters('ls_adls_mdcovidreportingdl_accountKey')]"
					},
					"ls_db_covid_cluster_accessToken": {
						"value": "[parameters('ls_db_covid_cluster_accessToken')]"
					},
					"ls_sql_covid_db_connectionString": {
						"value": "[parameters('ls_sql_covid_db_connectionString')]"
					},
					"ls_adls_mdcovidreportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_mdcovidreportingdl_properties_typeProperties_url')]"
					},
					"ls_http_ecdc_europa_eus_properties_typeProperties_url": {
						"value": "[parameters('ls_http_ecdc_europa_eus_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "mdcovidreportdf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_data_arrived_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_data_arrived_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mdcovidreportdf_ArmTemplate_0')]"
			]
		}
	]
}